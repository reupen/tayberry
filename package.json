{
  "name": "tayberry",
  "amdName": "Tayberry",
  "version": "0.4.2",
  "description": "Canvas-based bar and line charting library",
  "keywords": [
    "charts",
    "charting",
    "graphs",
    "graphing",
    "bar",
    "line",
    "canvas",
    "interactive"
  ],
  "homepage": "https://reupen.github.io/tayberry",
  "bugs": {
    "url": "https://github.com/reupen/tayberry"
  },
  "source": "src/tayberry.ts",
  "main": "dist/tayberry.js",
  "module": "dist/tayberry.mjs",
  "unpkg": "dist/tayberry.usm.js",
  "types": "dist/tayberry.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/reupen/tayberry.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@btmills/prettier": "2.1.1",
    "@storybook/addon-knobs": "^6.0.0-rc.14",
    "@storybook/addon-storysource": "^6.0.0-rc.14",
    "@storybook/addon-viewport": "^6.0.0-rc.14",
    "@storybook/client-api": "^6.0.0-rc.14",
    "@storybook/html": "^6.0.0-rc.14",
    "acorn": "^7.3.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jasmine-core": "^3.0.0",
    "karma": "^5.2.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.0.3",
    "karma-typescript-es6-transform": "^5.0.3",
    "lint-staged": "^10.2.11",
    "microbundle": "^0.12.3"
  },
  "scripts": {
    "build": "microbundle && tsc --declaration --emitDeclarationOnly --declarationDir dist src/tayberry.ts",
    "test": "karma start --single-run",
    "lint": "eslint .",
    "karma": "karma",
    "preversion": "npm run lint && npm test && npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,json,css,md}": "prettier --write"
  },
  "author": "Reupen Shah",
  "license": "MIT"
}
